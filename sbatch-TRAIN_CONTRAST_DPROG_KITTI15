#!/bin/bash
#SBATCH --workdir /home/tulyakov/research/mil-mc-cnn
#SBATCH --nodes 1
#SBATCH --ntasks 1
#SBATCH --cpus-per-task 1
#SBATCH --mem 16384
#SBATCH --time 24:00:00
#SBATCH --qos=gpu
#SBATCH --gres gpu:1
#SBATCH --partition=gpu
echo TRAIN CONTRAST_DPROG STARTING AT `date`
source /etc/profile.d/slmodules.sh -r deprecated
module purge
module load cuda/7.0
module load torch/7
module load opencv/3.0.0-rc1/gcc-4.4.7
th /home/tulyakov/research/mil-mc-cnn/TRAIN_NET.lua contrast-dprog kitti15 -debug_fname 'contrast-dprog-kitti15' -debug_start_from_fnet 'work/contrast-dprog-kitti15/fnet_2016_11_13_14:25:56_contrast-dprog-kitti15.t7'  -debug_start_from_optim 'work/contrast-dprog-kitti15/optim_2016_11_13_14:25:56_contrast-dprog-kitti15.t7'
echo FINISHED at `date`
